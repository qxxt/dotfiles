# ls aliases
alias ls='ls --group-directories-first --color=auto --time-style=long-iso -Fhv1'
alias l='ls -AgG'
alias ll='ls -Al'
# Sort by size
alias lt='ls -AsS1'
# Sort by time
# alias ltime='l -t'


# Copy with progress bar
alias cpv='rsync -ah --info=progress2'

# Go tour
alias tour="tour -openbrowser=f"
